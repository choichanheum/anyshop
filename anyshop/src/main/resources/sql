------- Member 테이블 생성 ------

CREATE TABLE member (
  id varchar(50) NOT NULL,
  pw varchar(20) NOT NULL,
  name varchar(50) NOT NULL,
  phone varchar(15) NOT NULL,
  gender varchar(5) NOT NULL,
  email varchar(50) NOT NULL,
  address varchar(100) NOT NULL,
  point int(100) default 0,
 
  PRIMARY KEY (id)
);

